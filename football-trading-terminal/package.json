{
  "name": "react-vite-tailwind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build --outDir dist",
    "lint": "npx tsc --noEmit && npx biome lint --write",
    "format": "npx biome format --write",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:migrate": "echo 'Run: supabase db push or apply supabase/migrations/001_architecture_freeze_v1.sql manually'",
    "ingest:historical": "bun run scripts/ingest/historical-download.ts",
    "ingest:daily": "bun run scripts/ingest/daily-incremental.ts",
    "ingest:live": "bun run scripts/ingest/live-update.ts",
    "ingest:live:once": "bun run scripts/ingest/live-update.ts --once"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.97.0",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-virtual": "^3.13.18",
    "lucide-react": "^0.575.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grab": "latest",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "same-runtime": "^0.0.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.27.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^18.3.22",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vercel/node": "^5.6.6",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "happy-dom": "^20.7.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vitest": "^4.0.18"
  }
}
